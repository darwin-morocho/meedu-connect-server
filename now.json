{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.ts",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/index.ts",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/rooms/create",
      "dest": "/src/api/routes/index.ts",
      "methods": ["POST"]
    }
  ],
  "public": false
}
